python - <<EOF
import json
users = []
for i in range(1,1001):
    users.append({
        "username": f"user{i}",
        "voucher": f"VC{i:04}",
        "status": "online" if i <= 30 else "offline"
    })
with open("users.json","w") as f:
    json.dump(users,f,indent=4)
print("1000 user berhasil dibuat. 30 online.")
EOF
